+#include <windows.h>
 #include <sspi.h>
 
 #define SC_IO_BUFFER_SIZE 0x4000
diff --git a/plugins/auth/sspi_client.c b/plugins/auth/sspi_client.c
index 75f5a1f5..2232022c 100644
--- a/plugins/auth/sspi_client.c
+++ b/plugins/auth/sspi_client.c
@@ -29,7 +29,7 @@ POSSIBILITY OF SUCH DAMAGE.
